{
  "tailwindCSS.config": "./tailwind.config.js",
  "tailwindCSS.experimental.classRegex": [
    ["(?:const|let|var)\\s+([a-zA-Z][a-zA-Z0-9]*)?(Styles|Variations)\\s*=\\s*['\"]([^'\"]*)['\"]", "'"],
    ["(?:const|let|var)\\s+([a-zA-Z][a-zA-Z0-9]*)?(Styles|Variations)\\s*=\\s*`([^`]*)`", "`"],
    ["(?:const|let|var)\\s+([a-zA-Z][a-zA-Z0-9]*)?(Styles|Variations)\\s*=\\s*\\[([\\s\\S]*?)\\]", "'"]
  ]
}